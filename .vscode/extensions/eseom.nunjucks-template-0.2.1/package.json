{
	"name": "nunjucks-template",
	"displayName": "Nunjucks Template",
	"description": "Formatting, Syntax Highlighting, Hover, and Snippets for Nunjucks",
	"version": "0.2.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/eseom/nunjucks-template"
	},
	"publisher": "eseom",
	"engines": {
		"vscode": "^1.19.0"
	},
	"categories": [
		"Programming Languages"
	],
	"icon": "images/icon.png",
	"activationEvents": [
		"onCommand:extension.sayHello",
		"onLanguage:njk"
	],
	"main": "./out/extension",
	"contributes": {
		"snippets": [
			{
				"language": "njk",
				"path": "./assets/snippets/snippets.json"
			}
		],
		"commands": [
			{
				"command": "extension.sayHello",
				"title": "Hello World"
			}
		],
		"languages": [
			{
				"id": "njk",
				"aliases": [
					"Nunjucks",
					"njk"
				],
				"extensions": [
					".njk"
				],
				"configuration": "./assets/languages/configuration.json"
			}
		],
		"grammars": [
			{
				"language": "njk",
				"scopeName": "source.njk",
				"path": "./assets/syntaxes/njk.json"
			},
			{
				"language": "njk",
				"scopeName": "text.html.njk",
				"path": "./assets/syntaxes/njk-html.json"
			}
		],
		"configuration": {
			"type": "object",
			"title": "nunjucks-template",
			"properties": {
				"nunjucks-template.sample": {
					"type": "boolean",
					"default": true,
					"description": "a sample configuration"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^7.0.43",
		"typescript": "^2.6.1",
		"vscode": "^1.1.35"
	},
	"dependencies": {
		"prettydiff2": "^2.2.7"
	},
	"__metadata": {
		"id": "ebdde13b-e0af-4d80-a28c-8a65daf6a554",
		"publisherId": "e3047e78-d502-451f-9f97-702734dc1a8e",
		"publisherDisplayName": "eseom"
	}
}